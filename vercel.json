{
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "https://rerest-dash.vercel.app/:path*"
    },
    {
      "source": "/admin",
      "destination": "https://rerest-dash.vercel.app/"
    },
    {
      "source": "/",
      "destination": "https://re-rest.vercel.app/"
    },
    {
      "source": "/:match*",
      "destination": "https://re-rest.vercel.app/:match*"
    },
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
